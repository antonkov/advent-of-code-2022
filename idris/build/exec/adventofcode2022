#!/bin/sh
# @generated by Idris 0.6.0-dbb385348, Chez backend

set -e # exit on any error

if [ "$(uname)" = Darwin ]; then
  DIR=$(zsh -c 'printf %s "$0:A:h"' "$0")
else
  DIR=$(dirname "$(readlink -f -- "$0")")
fi
export LD_LIBRARY_PATH="$DIR/adventofcode2022_app:$LD_LIBRARY_PATH"
export DYLD_LIBRARY_PATH="$DIR/adventofcode2022_app:$DYLD_LIBRARY_PATH"
export IDRIS2_INC_SRC="$DIR/adventofcode2022_app"

"$DIR/adventofcode2022_app/adventofcode2022.so" "$@"